@{
}
@model List<PositionViewModel>

<h1>Positions</h1>

<a class="btn btn-primary" href="@Url.Action("New", "Position")">Add New</a>

<table id="positionTable" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var dep in Model)
        {
            <tr>
                <td>@dep.Name</td>
            </tr>
        }
    </tbody>
</table>

